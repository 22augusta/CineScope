{
    "version": 2,
    "builds": [
        {
            "src": "MovieExplorer/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "^/assets/(.*)$",
            "dest": "/MovieExplorer/assets/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "^/$",
            "dest": "/MovieExplorer/index.html"
        },
        {
            "src": "^/(.*)$",
            "dest": "/MovieExplorer/index.html"
        }
    ]
}